<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CPR_VerticalSeekBar">
        <attr name="vsb_barBackgroundColor" format="color"/>
        <attr name="vsb_progressColor" format="color"/>
        <attr name="vsb_textColor" format="color"/>

        <attr name="vsb_barWidth" format="reference"/>
        <attr name="vsb_progress" format="integer"/>
        <attr name="vsb_max" format="integer"/>

        <attr name="vsb_textSize" format="dimension"/>
        <attr name="vsb_textMargin" format="dimension"/>

        <attr name="vsb_thumb" format="reference"/>
        <attr name="vsb_thumbWidth" format="dimension"/>
        <attr name="vsb_thumbHeight" format="dimension"/>

        <attr name="vsb_progressFromBottom" format="boolean"/>
        <attr name="vsb_textEnabled" format="boolean"/>
        <attr name="vsb_textSuffix" format="string"/>"
    </declare-styleable>
</resources>